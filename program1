Scanner sc=new Scanner(System.in);
 int n =sc.nextInt();
 int largest=Integer.MIN_VALUE;
 int smallest=Integer.MAX_VALUE;
 
 Systems.out.print f("Please enter %d numbers %n",n);
 for(int i=0;i<n;i++) {
 
 int current=sc.nextInt();
 if (current>largest) {
  largest=current;
 }
 if (current<smallest) { 
  smallest=current;
  
 }
}
